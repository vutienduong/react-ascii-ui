{
  "name": "react-ascii-ui",
  "version": "0.1.4",
  "description": "ASCII-styled React UI components for fun retro dashboards and apps",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vutienduong/react-ascii-ui.git",
    "directory": "packages/core"
  },
  "homepage": "https://react-ascii-ui-docs.vercel.app",
  "bugs": {
    "url": "https://github.com/vutienduong/react-ascii-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "dev": "tsup src/index.ts --watch",
    "prepare": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-onboarding": "^9.1.5",
    "@storybook/react-vite": "^9.1.5",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "storybook": "^9.1.5",
    "tailwindcss": "^3.4.0",
    "tsup": "^7.3.0",
    "typescript": "^5.5.0"
  },
  "keywords": [
    "ascii",
    "ui",
    "react",
    "tailwind",
    "retro",
    "cli",
    "tui"
  ],
  "author": "Mikel Vu",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
