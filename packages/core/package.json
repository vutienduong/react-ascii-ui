{
  "name": "react-ascii-ui",
  "version": "0.1.0",
  "description": "ASCII-styled React UI components for fun retro dashboards and apps",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "repository": {
    "type": "git",
    "url": "https://github.com/vutienduong/react-ascii-ui.git",
    "directory": "packages/core"
  },
  "homepage": "https://react-ascii-ui.dev",
  "bugs": {
    "url": "https://github.com/vutienduong/react-ascii-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "dev": "tsup src/index.ts --watch",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "typescript": "^5.5.0",
    "tsup": "^7.3.0",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0"
  },
  "keywords": ["ascii", "ui", "react", "tailwind", "retro", "cli", "tui"],
  "author": "Mikel Vu",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
